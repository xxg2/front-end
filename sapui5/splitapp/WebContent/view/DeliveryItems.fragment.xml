<core:FragmentDefinition xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:f="sap.ui.layout.form">
<f:SimpleForm title="Delivery Schedules">
	<IconTabBar id="idIconTabBarSeparatorMixed"
		expanded="false" expandable="false">
		<items>
			<IconTabFilter count="15" text="Schedule(s)" showAll="true" key="All"/>
			<IconTabSeparator icon="" />
			<IconTabFilter icon="sap-icon://task" iconColor="Critical" text="Open" design="Horizontal"
				count="3">
				<Text text="Attachments go here ..." />
			</IconTabFilter>
			<IconTabFilter icon="sap-icon://process" iconColor="Negative" text="In Progress" design="Horizontal"
				count="12">
				<Text text="Notes go here ..." />
			</IconTabFilter>
			<IconTabFilter icon="sap-icon://shipping-status" iconColor="Positive" text="Shipped" design="Horizontal">
				<Text text="People content goes here ..." />
			</IconTabFilter>
		</items>
	</IconTabBar>
</f:SimpleForm>
</core:FragmentDefinition>